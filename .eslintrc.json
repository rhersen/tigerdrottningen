{
  "plugins": ["prettier"],
  "env": {
    "browser": true,
    "mocha": true,
    "node": true
  },
  "extends": ["eslint:recommended"],
  "rules": {
    "prettier/prettier": ["error"],
    "prefer-arrow-callback": ["error"],
    "prefer-const": ["error"],
    "prefer-template": ["error"]
  },
  "parser": "babel-eslint"
}
